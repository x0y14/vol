# The underscore in the name is for clarity.
# It does not have any special meaning.
# Comments are from after # to the end of line.


__set_values:
  set_reg_a 0 # [0, 1]
  set_reg_b 0 # [2, 3]


__if_else:
  ## if (a == b) -> set_reg_c = 2
  ## else        -> set_reg_c = 3

  compare_a_and_b # [4]
  jump_eq __if # [5, 6]


__else:
  set_reg_c 3 # [7, 8]

__goto_last:
  jump __last # [9, 10]


__if:
  set_reg_c 2 # [11, 12]


__last:
  set_reg_a 4 # [13, 14]

#__exit:
  exit # [15]